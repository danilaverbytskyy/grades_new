{
  "name": "template-vue3-base",
  "version": "1.3.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run || exit 0",
    "test:e2e": "playwright test",
    "test:e2e:stress": "playwright test -c playwright/config/pw-stress.config.js",
    "test:e2e:ci": "playwright test -c playwright/config/pw-ci.config.js || exit 0",
    "lint": "eslint . --fix",
    "playwright:install": "npm exec playwright install --with-deps firefox",
    "check-types": "vue-tsc --noEmit",
    "i18n:report": "node i18n-report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"  --output \"i18n-report.json\" "
  },
  "dependencies": {
    "@primeuix/themes": "1.0.0",
    "@primevue/forms": "4.3.2",
    "pinia": "3.0.1",
    "primeflex": "4.0.0",
    "primeicons": "7.0.0",
    "primevue": "4.3.2",
    "quill": "2.0.3",
    "vue": "3.5.13",
    "vue-i18n": "11.1.2",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@pinia/testing": "1.0.0",
    "@playwright/test": "1.51.0",
    "@types/node": "22.13.10",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/test-utils": "2.4.6",
    "dotenv": "16.4.7",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "10.0.0",
    "jsdom": "26.0.0",
    "prettier": "3.5.3",
    "sass": "1.85.1",
    "typescript": "5.8.2",
    "vite": "6.2.1",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "3.0.8",
    "vue-i18n-extract": "2.0.7",
    "vue-tsc": "2.2.8"
  }
}
